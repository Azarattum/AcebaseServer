  "author": "Ewout Stortenbeker <me@appy.one> (http://appy.one)",
  "license": "MIT",
  "dependencies": {
    "acebase": "^1.15.3",
    "acebase-core": "^1.13.1",
    "express": "^4.17.1",
    "socket.io": "^2.4.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^14.0.14",
