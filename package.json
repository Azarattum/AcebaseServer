{
  "name": "acebase-server",
  "version": "1.9.1",
  "description": "AceBase realtime database server (webserver endpoint to allow remote connections)",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "private": false,
  "repository": "github:appy-one/acebase-server",
